  function doQuery() {
    e =   document.queryForm;
    if (e.beginTime.value > e.endTime.value){
        alert("开始时间不能小于结束时间");
        return false;
    }
    document.queryForm.submit();
}

function clearQueryCondition(){
   e = document.queryForm ;
   e.title.value= '';
   e.beginTime.value= '';
   e.endTime.value='';
   e.sender.value='';
}

